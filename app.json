{
  "expo": {
    "entryPoint": "./src/components/App.js",
    "name": "DanceBlue Mobile",
    "slug": "danceblue-mobile",
    "privacy": "unlisted",
    "owner": "university-of-kentucky-danceblue",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash2.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.danceblue.dancebluemarathon"
    },
    "android": {
      "package": "com.danceblue.dancebluemarathon",
      "versionCode": 1,
      "permissions": [
        "WRITE_CALENDAR",
        "READ_CALENDAR"
      ]
    },
    "description": "University of Kentucky DanceBlue Mobile Application"
  }
}
